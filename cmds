export http_proxy=http://192.168.195.1:7890
export https_proxy=http://192.168.195.1:7890
export ftp_proxy=http://192.168.195.1:7890
export ALL_PROXY=socks5://192.168.195.1:7889
export all_proxy=socks5://192.168.195.1:7889

RUN git clone https://github.com/gcc-mirror/gcc.git
RUN cd gcc
RUN git checkout releases/gcc-12.2.0
RUN ./contrib/download_prerequisites
RUN cd ..
RUN mkdir objdir
RUN cd objdir
RUN $PWD/../gcc-12.2.0/configure --prefix=$HOME/GCC-12.2.0 --enable-languages=c,c++,fortran,go
RUN make -j
RUN make install

